                    </box>
                </geometry>
                <material>
                    <script>
                        <uri>file://media/materials/scripts/gazebo.material</uri>
                        <name>Gazebo/Green</name>
                    </script>
                </material>
            </visual>
        </link>
        <link name="hinge_link">
            <pose>-0.725 0 0.85 0 0 0</pose>
            <inertial>
                <mass>1000</mass>
            </inertial>
            <collision name="hinge_collision">
